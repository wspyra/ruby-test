<!DOCTYPE html>
<html>
<head>
  <title>RubyTest</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%- flash.each do |name, message| %>
<p><i><%= "#{name}: #{message} "%></i></p>
<% end %>

<%= yield %>

</body>
</html>
