<table>
  <tr>
    <th><%=t 'position' %></th>
    <th><%=t 'name' %></th>
    <th><%=t 'visible' %></th>
    <th><%=t 'created' %></th>
    <th><%=t 'updated' %></th>
    <th colspan="3"><%=t 'actions' %></th>
  </tr>
    <% @tests.each do |t| %>
        <tr>
          <td><%= t.position %></td>
          <td><%= t.name %></td>
          <td><%= t.visible ? t('yes_label') : t('no_label') %></td>
          <td><%=l t.created_at %></td>
          <td><%=l t.updated_at %></td>
          <td><%= link_to(t('show'), {:action => 'show', :id => t.id}) %></td>
          <td><%= link_to(t('edit'), {:action => 'edit', :id => t.id}) %></td>
          <td><%= link_to(t('delete'), {:action => 'delete', :id => t.id}) %></td>
        </tr>
    <% end %>
</table>
<%= will_paginate @tests %>

<%= link_to(t('add_new'), {:action => 'new'}) %>

